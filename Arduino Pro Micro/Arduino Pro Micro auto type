#include <Keyboard.h>

String text = "These are the words I want it to type.";  // Define your text

void setup() {
  delay(1000);  // Short delay to ensure the system is ready
  Keyboard.begin();
  Keyboard.print(text);  // Type the stored text
  Keyboard.end();  // Stops keyboard after sending text
}

void loop() {
  // Nothing here, as we only want to send the message once
}These are the words I want it to type.